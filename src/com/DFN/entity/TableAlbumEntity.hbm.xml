<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.DFN.entity.TableAlbumEntity" table="table_album" schema="dfn">
        <id name="albumId" column="album_id"/>
        <property name="albumName" column="album_name"/>
        <property name="albumDate" column="album_date"/>
        <property name="albumOther" column="album_other"/>
    </class>
</hibernate-mapping>