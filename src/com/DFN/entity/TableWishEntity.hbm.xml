<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.DFN.entity.TableWishEntity" table="table_wish" schema="dfn">
        <id name="wishId" column="wish_id"/>
        <property name="wishName" column="wish_name"/>
        <property name="wishPublic" column="wish_public"/>
        <property name="wishState" column="wish_state"/>
        <property name="wishOther" column="wish_other"/>
    </class>
</hibernate-mapping>