<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.DFN.entity.TableShareEntity" table="table_share" schema="dfn">
        <id name="shareId" column="share_id"/>
        <property name="shareName" column="share_name"/>
        <property name="shareContent" column="share_content"/>
        <property name="shareDate" column="share_date"/>
        <property name="shareOther" column="share_other"/>
    </class>
</hibernate-mapping>