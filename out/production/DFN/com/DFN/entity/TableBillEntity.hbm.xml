<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.DFN.entity.TableBillEntity" table="table_bill" schema="dfn">
        <id name="billId" column="bill_id"/>
        <property name="billGoods" column="bill_goods"/>
        <property name="billPrice" column="bill_price"/>
        <property name="billDate" column="bill_date"/>
        <property name="billOther" column="bill_other"/>
    </class>
</hibernate-mapping>